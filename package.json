    "test-dev": "karma start",
    "postinstall": "babel ./src -d lib"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.3.2",
    "classnames": "^2.2.0",
    "es5-shim": "^4.1.14",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "history": "^1.12.5",
    "jasmine-core": "^2.3.4",
    "jasmine-es6": "0.0.18",
    "karma": "^0.13.11",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs2-launcher": "^0.3.2",
    "karma-webpack": "^1.7.0",
    "lolex": "^1.3.2",
    "marked": "^0.3.5",
    "phantomjs": "^1.9.18",
