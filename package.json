{
  "name": "react-regions",
  "description": "Region rendering solution for react",
  "version": "0.2.1",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/scup/react-regions.git"
  },
  "directories": {
    "example": "examples",
    "docs": "docs",
    "test": "test"
  },
  "scripts": {
